{
  "name": "vitest-gen",
  "type": "module",
  "version": "0.1.4",
  "description": "Package for generating NextJS template files with corresponding Vitest test files",
  "author": "Kristian Kolehmainen <kristian@koleman.dev>",
  "license": "MIT",
  "homepage": "https://github.com/MCKoleman/vitest-gen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MCKoleman/vitest-gen.git"
  },
  "bugs": {
    "url": "https://github.com/MCKoleman/vitest-gen/issues"
  },
  "keywords": [
    "NextJS",
    "vitest",
    "generate",
    "template",
    "test"
  ],
  "engines": {
    "node": "^18.0.0  || >=20.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "vitest-gen": "./dist/index.js"
  },
  "scripts": {
    "vitest-gen": "tsx src/index.ts",
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "jsdom": "^25",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/dom": "^10",
    "@testing-library/react": "^16",
    "@types/node": "^18.0.0 || >=20.0.0",
    "@types/react": "^18",
		"@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4",
    "tsup": "^8",
    "tsx": "^4",
    "typescript": "^5",
    "vitest": "^2.1.1"
  }
}
